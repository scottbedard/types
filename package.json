{
  "author": "Scott Bedard",
  "bugs": {
    "url": "https://github.com/scottbedard/types/issues"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "homepage": "https://github.com/scottbedard/types#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@bedard/types",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottbedard/types.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json --outDir dist/",
    "copy-dts": "copyfiles -u 1 \"src/**/*.d.ts\" dist",
    "postbuild": "npm run copy-dts",
    "prebuild": "rm -rf dist/",
    "publish:ci": "node ./scripts/publish.js",
    "test": "tsc tests/*.ts --listFiles --noEmit"
  },
  "version": "0.2.2"
}
